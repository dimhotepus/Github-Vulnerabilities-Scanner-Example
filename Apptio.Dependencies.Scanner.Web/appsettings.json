{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "RESILIENCE": {
    "Http": {
      "Timeout": {
        "Timeout": "00:00:10",
        "Enabled": "true"
      },
      "CircuitBreaker": {
        "ExceptionsAllowedBeforeBreaking": "3",
        "DurationOfBreak": "00:00:15",
        "Enabled": "true"
      },
      "Retry": {
        "Retries": "3",
        "Enabled": "true"
      }
    }
  }
}
